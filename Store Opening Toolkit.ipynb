    "### Get Popular Times Data from Google Places"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {
    "id": "5cc7419b-252d-4b1b-baef-069892147844"
   },
   "outputs": [],
   "source": [
    "def get_popular_times(place_id):\n",
    "    response = requests.get(\"https://maps.googleapis.com/maps/api/place/details/json\",\n",
    "    params = {\n",
    "        \"place_id\": place_id,\n",
    "        \"fields\": \"name,current_opening_hours\",\n",
    "        \"key\":\"AIzaSyAb6I_EOAaoey1rQJ12Sy26AgJtx0kv354\"\n",
    "    })\n",
    "    results = response.json()\n",
    "\n",
    "    if \"result\" in results and \"current_opening_hours\" in results[\"result\"]:\n",
    "        return results[\"result\"][\"current_opening_hours\"]\n",
    "    return None"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {
    "id": "8c9b011b-1444-4ec9-909c-a1b2986d86e8"
   },
   "outputs": [],
   "source": [
    "## Fetch Popular Times for each place\n",
    "for place in sf_places:\n",
    "    place[\"current_opening_hours\"] = get_popular_times(place[\"place_id\"])\n",
    "    time.sleep(1)  ## Prevent API rate limits"
   ]
